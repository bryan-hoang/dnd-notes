{
  "name": "dnd-notes",
  "private": true,
  "description": "Notes on my sessions of D&amp;D as a player.",
  "homepage": "https://github.com/bryan-hoang/dnd-notes#readme",
  "bugs": {
    "url": "https://github.com/bryan-hoang/dnd-notes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryan-hoang/dnd-notes.git"
  },
  "license": "ISC",
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "format": "prettier -c .",
    "lint": "eslint --cache --ext ts,tsx,js .",
    "markdownlint": "markdownlint .",
    "prepare": "husky install",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "test": "pnpm run markdownlint",
    "typecheck": "tsc",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-rc.1",
    "@docusaurus/preset-classic": "^2.0.0-rc.1",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^6.3.0",
    "clsx": "^1.2.1",
    "file-loader": "^6.2.0",
    "hast-util-is-element": "2.1.2",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-katex": "4.0.0",
    "remark-math": "3.0.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@docusaurus/module-type-aliases": "^2.0.0-rc.1",
    "@tsconfig/docusaurus": "^1.0.6",
    "@tsconfig/node14": "1.0.3",
    "@types/react": "^18.0.15",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "13.0.3",
    "markdownlint-cli": "0.32.0",
    "prettier": "2.7.1",
    "typescript": "^4.7.4"
  }
}
