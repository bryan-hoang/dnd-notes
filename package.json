		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-import": "2.28.1",
		"eslint-plugin-prettier": "4.2.1",
		"husky": "8.0.3",
		"lint-staged": "15.0.0",
		"markdownlint-cli": "0.37.0",
		"prettier": "2.8.8",
		"typescript": "5.2.2"
	},
	"pnpm": {
		"overrides": {
			"got@<11.8.5": ">=11.8.5",
			"semver": ">=7.5.2",
			"trim@<0.0.3": ">=0.0.3",
			"word-wrap": "npm:@aashutoshrathi/word-wrap"
		},
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "18",
				"react-dom": "18"
			}
		}
